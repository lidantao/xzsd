<!--&lt;!&ndash;        </if>&ndash;&gt;-->

<!--&lt;!&ndash;    </select>&ndash;&gt;-->
<!--&lt;!&ndash;&lt;!&ndash;    查询菜单名是否存在&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;    <select id="checkMenuName" parameterType="com.xzsd.pc.menu.entity.MenuInfo" resultType="int">&ndash;&gt;-->
<!--&lt;!&ndash;        select  count(menu_code)&ndash;&gt;-->
