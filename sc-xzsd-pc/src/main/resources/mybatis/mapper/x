<!--&lt;!&ndash;        last_update_user    =       #{lastUpdateUser},&ndash;&gt;-->
<!--&lt;!&ndash;        last_update_time    =       now()&ndash;&gt;-->
<!--&lt;!&ndash;        where   menu_code   =       #{menuCode}&ndash;&gt;-->
<!--&lt;!&ndash;        and     version     =       #{version}&ndash;&gt;-->
<!--&lt;!&ndash;    </update>&ndash;&gt;-->
