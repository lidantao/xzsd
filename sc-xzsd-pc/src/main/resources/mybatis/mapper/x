<!--&lt;!&ndash;        and     is_delete   =   0&ndash;&gt;-->
<!--&lt;!&ndash;    </select>&ndash;&gt;-->
<!--&lt;!&ndash;&lt;!&ndash;    新增菜单接口&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;    <insert id="addMenu" parameterType="com.xzsd.pc.menu.entity.MenuInfo">&ndash;&gt;-->
