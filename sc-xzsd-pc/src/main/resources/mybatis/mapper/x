<!--&lt;!&ndash;        values&ndash;&gt;-->
<!--&lt;!&ndash;        (&ndash;&gt;-->
<!--&lt;!&ndash;        #{menuCode},        #{menuName},        #{menuPath},&ndash;&gt;-->
<!--&lt;!&ndash;        #{menuRemark},      #{isDelete},        #{createUser},&ndash;&gt;-->
<!--&lt;!&ndash;        now(),              #{lastUpdateUser},  now(),&ndash;&gt;-->
<!--&lt;!&ndash;        0&ndash;&gt;-->
<!--&lt;!&ndash;        )&ndash;&gt;-->
<!--&lt;!&ndash;    </insert>&ndash;&gt;-->
<!--&lt;!&ndash;&lt;!&ndash;    查询菜单详情接口&ndash;&gt;&ndash;&gt;-->
